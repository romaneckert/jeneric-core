{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["app", "common", "web", "README.md"],
        "exclude": [],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "recurse": true,
        "private": false,
        "encoding": "utf8",
        "destination": "./doc",
        "template": "node_modules/minami"
    }
}