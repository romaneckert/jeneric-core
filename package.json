{
    "name": "@jeneric/core",
    "version": "2.0.7",
    "description": "jeneric core",
    "main": "index.js",
    "scripts": {
        "start": "node bin/install.js && node .",
        "test": "export NODE_ENV=test && node bin/install.js && nyc --all --reporter=text mocha",
        "docs:build": "./node_modules/documentation/bin/documentation.js build src/** -f html -o docs"
    },
    "author": "Roman Eckert",
    "license": "MIT",
    "repository": "https://github.com/romaneckert/jeneric-core",
    "dependencies": {
        "@jeneric/app": "2.0.1",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "cookie-parser": "1.4.4",
        "express": "4.16.4",
        "helmet": "3.17.0",
        "jsonwebtoken": "8.5.1",
        "mongoose": "5.5.5",
        "nodemailer": "6.1.1",
        "nodemon": "^1.19.0",
        "pug": "2.0.3",
        "stack-trace": "0.0.10"
    },
    "nodemonConfig": {
        "ignore": [
            "public/*",
            "node_modules/@jeneric/app"
        ],
        "delay": "100",
        "ignoreRoot": [
            ".git"
        ],
        "ext": "js,json,pug,css",
        "exec": "npm run start"
    },
    "nyc": {
        "all": true,
        "include": [
            "node_modules/@jeneric/app"
        ],
        "excludeNodeModules": false,
        "temp-directory": "var/nyc"
    },
    "devDependencies": {
        "documentation": "10.1.0",
        "eslint": "5.16.0",
        "eslint-plugin-jsdoc": "4.8.3",
        "mocha": "6.1.4",
        "nyc": "14.1.0"
    }
}
